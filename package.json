{
  "name": "regexp-to-ast",
  "version": "0.0.1",
  "main": "lib/parser.js",
  "repository": "https://github.com/bd82/regexp-to-ast.git",
  "author": "Shahar Soel",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-es5": "^1.3.1",
    "mocha": "^5.0.5",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "prettier": "1.11.1",
    "typescript": "^2.8.1"
  },
  "scripts": {
    "ci_full_flow": "npm-run-all type_check test check_coverage verify_format",
    "test": "nyc mocha \"./test/**/*spec.js\"",
    "check_coverage": "nyc check-coverage --lines 100 --statements 100 --functions 100",
    "format": "prettier --no-editorconfig --write \"**/*.@(ts|js)\"",
    "verify_format": "prettier --no-editorconfig --list-different  \"**/*.@(ts|js)\"",
    "type_check": "tsc"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "test/**.*"
    ]
  },
  "prettier": {
    "semi": false,
    "tabWidth": 4,
    "useTabs": false
  },
  "dependencies": {}
}
